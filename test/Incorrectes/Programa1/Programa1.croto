crotofunc main(){
    int a;
    int b;
    scan(a);
    scan(b);
    print(modulo(a,b));


crotofunc int modulo(int a, int b){  
    int quocient;
    if(b<=0){
        return -1;
    }
    quocient = a/b;
    return a-b*quocient;
}

